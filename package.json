{
    "name": "@slite/slite-dev-test",
    "version": "1.0.0",
    "description": "Slite test for new developers",
    "main": "src/index.js",
    "directories": {
        "test": "test"
    },
    "watch": {
        "start": "{src,test}/*.js"
    },
    "scripts": {
        "watch": "npm-watch",
        "build": "babel -d dist/ src/",
        "start": "npm run build;node dist/index.js",
        "test": "jest ./test.js",
        "postinstall": "mkdir ./notes"
    },
    "keywords": [
        "Slite",
        "dev",
        "test"
    ],
    "author": "Slite Team",
    "license": "ISC",
    "dependencies": {
        "jest": "^23.1.0",
        "underscore.string": "^3.3.4"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "npm-watch": "^0.3.0",
        "prettier": "1.13.4"
    }
}
